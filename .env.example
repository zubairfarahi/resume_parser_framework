# Resume Parser Framework - Environment Configuration
# Copy this file to .env and update with your API key

# ============================================================================
# OpenAI Configuration (REQUIRED for Skills Extraction)
# ============================================================================
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_TEMPERATURE=0.0

# ============================================================================
# File Processing Settings (Optional - has sensible defaults)
# ============================================================================
RESUME_PARSER_MAX_FILE_SIZE=10485760  # 10MB in bytes
RESUME_PARSER_PARSING_TIMEOUT=30      # seconds

# ============================================================================
# Logging Settings (Optional)
# ============================================================================
RESUME_PARSER_LOG_LEVEL=INFO          # DEBUG, INFO, WARNING, ERROR, CRITICAL
RESUME_PARSER_LOG_FORMAT=console      # console or json
